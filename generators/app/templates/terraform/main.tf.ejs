provider "aws" {
  region            = "<%=region%>"
  assume_role {
    role_arn = "arn:aws:iam::${var.account}:role/terraform"
  }
}

terraform {
  backend "s3" {
    encrypt               = false
    region                = "<%=region%>"
    bucket                = "foo-tf"
    dynamodb_table        = "terraform-lock"
    key                   = "terraform-state"
    workspace_key_prefix  = "<%=project%>"
  }
}
