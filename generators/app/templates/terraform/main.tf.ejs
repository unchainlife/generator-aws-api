provider "aws" {
  region            = "<%=region%>"
  profile           = "default"
}

terraform {
  backend "s3" {
    encrypt               = false
    region                = "<%=region%>"
    bucket                = "foo-tf"
    dynamodb_table        = "terraform-lock"
    key                   = "terraform-state"
    workspace_key_prefix  = "<%=project%>"
  }
}
