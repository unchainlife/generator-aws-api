resource "aws_api_gateway_resource" "<%=api%>_<%=fullname%>" {
    rest_api_id = aws_api_gateway_rest_api.<%=api%>.id
    parent_id   = <%=
        isNone(parent)
            ? `aws_api_gateway_rest_api.${api}.root_resource_id`
            : `aws_api_gateway_resource.${api}_${parent}.id`
    %>
    path_part   = "<%=pathpart%>"
}
